   <select id="getHttpRequestLog" parameterType="map" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from deayou_http_request_log
    where 1=1
      <if test="requestType != null">
      	 and request_type = #{requestType,jdbcType=VARCHAR}
      </if>
      <if test="pagenum != null">
       	and  request_url  like concat('%',#{pagenum},'%')
      </if>
      <if test="pagesize != null">
	  	 and  request_url  like concat('%',#{pagesize},'%')
	  </if>
      <if test="platcust != null">
	  	 and  request_url  like concat('%',#{platcust},'%')
	  </if>
	  <if test="start_date != null">
	  	 and  request_url  like concat('%',#{start_date},'%')
	  </if>
	  <if test="end_date != null">
	  	 and  request_url  like concat('%',#{end_date},'%')
	  </if>
      <if test="requestTime != null">
	  	 and FROM_UNIXTIME(`request_time`/1000, '%Y-%m-%d') = #{requestTime,jdbcType=VARCHAR} 
	  </if>
    order by id desc limit 1
    
  </select>


  <select id="getRecoverHttpRequestLog" resultMap="BaseResultMap">
    select log.order_id FROM  deayou_http_request_log log
    where request_type = 'request_url_bidding_repay'  and #{addtime}*1000-request_time BETWEEN -10000 and 10000  and REPLACE(substr(`request_url`, LOCATE('&amp;prod_id=', `request_url`)+ 9), substr(`request_url`, LOCATE('&amp;repay_flag=', `request_url`)), '') =
        (select borrow_nid from `depository_borrow_recover` where id =#{recoverId})
        and local_error is null limit 0,1
  </select>
  
